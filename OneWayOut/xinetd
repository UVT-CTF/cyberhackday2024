service owo
{
    disable = no
    type        = UNLISTED
    wait        = no
    server      = /bin/sh
    server_args = -c cd${IFS}/home/solver/;exec${IFS}./run.sh
    socket_type = stream
    protocol    = tcp
    user        = solver
    port        = 8889
    # bind        = 0.0.0.0
    # safety options
    flags       = REUSE
    per_source	= 10 # the maximum instances of this service per source IP address
    rlimit_cpu	= 1 # the maximum number of CPU seconds that the service may use
    nice        = 18
}